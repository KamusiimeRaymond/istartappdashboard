<section data-bs-version="5.1" class="header6 cid-ukWVLQSNmQ" id="header6-r">
    <div class="mbr-overlay" style="opacity: 0.8; background-color: rgb(255, 145, 0);"></div>
    <div class="align-center container">
        <div class="row justify-content-center">
            <div class="col-lg-6 col-md-6 col-sm-6">
                <h1 class="mbr-section-title mbr-fonts-style mbr-white mb-3 display-7"><strong>Partners</strong></h1>
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <form action="gotoaddpartnerform" method="POST">
                    <input hidden name="token" data-form-field="token" value="<%=token %>">
                    <button type="submit" class="addgrantbutton display-4" href="">Add Partner</button>
                </form>
            </div>
        </div>
    </div>
</section>

<section>  
    <div class="container">
        <div class="justify-content-center content-row mt-4">
            <div class="card p-4 p-md-3 col-lg-12 col-md-12">
                <div class="card-box">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            <div class="table-responsive">
                                <table class="table" id="dataTable">
                                    <thead class="display-4">
                                        <tr>
                                            <th>Partner Profile</th>
                                            <th>Name</th>
                                            <th>Location</th>
                                            <th>Type</th>
                                            <th>Country</th>
                                            <th colspan="2">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody class="display-4 tableData">
                                        <% partners.forEach(function(item){ %>
                                        <tr>
                                            <td><img src="<%=item.imageIcon %>" alt="PartnerImageIcon" style="width:35px;height:35px;border-radius:35px;"></td>
                                            <td><%=item.name %></td>
                                            <td><%=item.location %></td>
                                            <td><%=item.type %></td>
                                            <td><%=item.country %></td>
                                            <td>
                                                <form action="viewpartner" method="POST">
                                                    <input hidden name="token" data-form-field="token" value="<%=token %>">
                                                    <input hidden name="partnerId" data-form-field="partnerId" value="<%=item._id %>">
                                                    <button type="submit" class="view" href="">View</button>
                                                </form>
                                            </td>
                                            <td>
                                                <form action="deletepartner" method="POST">
                                                    <input hidden name="token" data-form-field="token" value="<%=token %>">
                                                    <input hidden name="partnerId" data-form-field="partnerId" value="<%=item._id %>">
                                                    <button type="submit" class="delete" href="">Delete</button>
                                                </form>
                                            </td>
                                        </tr> 
                                        <% }) %>   
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>  
        </div> 
    </div>
</section>

<section data-bs-version="5.1" class="footer7 cid-ukWNFCKGag" once="footers" id="footer7-b">
