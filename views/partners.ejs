<% if(typeof message!= "undefined" && message && status){ %>
<section class="infoupdated">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-12">
                <h3 class="mbr-section-title mb-4 mbr-fonts-style display-4 updatedMessage"><strong><%=message %></strong></h3>
            </div>
        </div>
    </div>
</section>
